{
  "name": "heatmapper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "develop": "../serve.sh"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "core-js": "^3.6.5",
    "express": "^4.0.0",
    "express-ws": "^4.0.0",
    "mapbox-gl": "^1.11.0",
    "mapbox-gl-vue": "^2.0.4",
    "vue": "^2.6.11",
    "vue-head": "^2.2.0",
    "vue-simple-spinner": "^1.2.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "prettier": {
    "printWidth": 100,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended",
      "plugin:prettier/recommended",
      "@vue/airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "babel"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "vue/html-self-closing": [
        "error",
        {
          "html": {
            "void": "always"
          }
        }
      ],
      "vue/html-indent": "off",
      "vue/html-closing-bracket-newline": "off",
      "vue/max-attributes-per-line": "off",
      "max-len": "warn",
      "no-unused-expressions": "off",
      "babel/no-unused-expressions": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
